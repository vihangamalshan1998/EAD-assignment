<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/create_order_three_frag"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.fuelstation.admin.CreateStation">


    <androidx.constraintlayout.widget.ConstraintLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="5dp">


        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#b2b3ad"
            android:gravity="center_horizontal"
            android:text="@string/create_fuel_station"
            android:textAlignment="center"
            android:textSize="34sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.007"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="48dp"
            android:text="@string/station_name"
            android:textColor="#070707"
            app:layout_constraintBottom_toBottomOf="@+id/create_station_name_id"
            app:layout_constraintEnd_toEndOf="@+id/create_station_name_id"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/create_station_name_id"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="28dp"
            android:text="@string/close_time"
            android:textColor="#070707"
            app:layout_constraintEnd_toEndOf="@+id/create_station_close_time_id"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/create_station_close_time_id"
            app:layout_constraintTop_toBottomOf="@+id/create_station_opent_time_id"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:text="@string/open_time"
            android:textColor="#070707"
            app:layout_constraintEnd_toEndOf="@+id/create_station_opent_time_id"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/create_station_opent_time_id"
            app:layout_constraintTop_toBottomOf="@+id/create_station_location_id"
            tools:ignore="MissingConstraints" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="28dp"
            android:text="@string/station_location"
            android:textColor="#090909"
            app:layout_constraintEnd_toEndOf="@+id/create_station_location_id"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/create_station_location_id"
            app:layout_constraintTop_toBottomOf="@+id/create_station_name_id"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/create_station_btn"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="28dp"
            android:text="@string/create_fuel_station"
            app:backgroundTint="#F8EA70"
            android:textColor="@android:color/black"
            app:layout_constraintEnd_toEndOf="@+id/create_station_close_time_id"
            app:layout_constraintHorizontal_bias="0.58"
            app:layout_constraintStart_toStartOf="@+id/create_station_close_time_id"
            app:layout_constraintTop_toBottomOf="@+id/create_station_close_time_id"
            tools:ignore="MissingConstraints" />

        <EditText
            android:id="@+id/create_station_close_time_id"
            android:layout_width="300dp"
            android:layout_height="48dp"
            android:layout_marginTop="44dp"
            android:background="@drawable/border_view_bg"
            android:ems="10"
            android:inputType="textCapSentences|textAutoCorrect|textMultiLine"
            android:textAlignment="center"
            android:textColor="@color/black"
            app:layout_constraintEnd_toEndOf="@+id/create_station_opent_time_id"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/create_station_opent_time_id"
            app:layout_constraintTop_toBottomOf="@+id/create_station_opent_time_id" />

        <EditText
            android:id="@+id/create_station_name_id"
            android:layout_width="300dp"
            android:layout_height="48dp"
            android:textAlignment="center"
            android:layout_marginTop="76dp"
            android:background="@drawable/border_view_bg"
            android:inputType="textCapSentences|textAutoCorrect|textMultiLine"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.504"
            app:layout_constraintStart_toStartOf="@+id/textView3"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            tools:ignore="SpeakableTextPresentCheck" />

        <EditText
            android:id="@+id/create_station_location_id"
            android:layout_width="300dp"
            android:layout_height="48dp"
            android:layout_marginTop="48dp"
            android:background="@drawable/border_view_bg"
            android:inputType="textCapSentences|textAutoCorrect|textMultiLine"
            android:textAlignment="center"
            app:layout_constraintEnd_toEndOf="@+id/create_station_name_id"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/create_station_name_id"
            app:layout_constraintTop_toBottomOf="@+id/create_station_name_id"
            tools:ignore="SpeakableTextPresentCheck" />

        <EditText
            android:id="@+id/create_station_opent_time_id"
            android:layout_width="300dp"
            android:layout_height="48dp"
            android:layout_marginTop="40dp"
            android:background="@drawable/border_view_bg"
            android:ems="10"
            android:inputType="textCapSentences|textAutoCorrect|textMultiLine"
            android:textAlignment="center"
            android:textColor="@color/black"
            app:layout_constraintEnd_toEndOf="@+id/create_station_location_id"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/create_station_location_id"
            app:layout_constraintTop_toBottomOf="@+id/create_station_location_id" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>